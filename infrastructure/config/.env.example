# Enclave identity.
ENCLAVE_ENROLMENT_KEY=replace-with-enclave-enrolment-key

# Drupal + MariaDB credentials.
DRUPAL_DB_NAME=drupal
DRUPAL_DB_USER=drupal
DRUPAL_DB_PASSWORD=change-me-drupal
MARIADB_ROOT_PASSWORD=change-me-root

# Grafana admin credentials.
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=change-me-grafana

# Optional: leave S3_BUCKET empty to use local named volume for sites/default/files.
S3_BUCKET=
S3_PREFIX=sites/default/files
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=

# IPFS enclave settings for auditable OSINT trust objects.
IPFS_API_HOST=127.0.0.1
IPFS_API_PORT=5001
IPFS_SWARM_PORT=4001
ENCLAVE_VIF_ADDR=100.96.0.10
HSM_KEY_ID=ato-ed25519
